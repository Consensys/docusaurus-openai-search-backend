{
  "name": "docusaurus-openai-search-proxy",
  "version": "1.0.0",
  "description": "Simple secure backend proxy for Docusaurus OpenAI Search plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && mkdir -p api && cp dist/index.js api/index.js",
    "vercel-build": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "keywords": ["docusaurus", "openai", "proxy", "search"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "openai": "^4.24.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
